<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="ogh_mockup.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="/LabelSetControl.axaml"></StyleInclude>
    </Application.Styles>

	<Application.Resources>
		<SolidColorBrush x:Key="MidGreyBrush">#373c44</SolidColorBrush>
		<SolidColorBrush x:Key="LightGreyBrush">#606875</SolidColorBrush>
		<SolidColorBrush x:Key="DarkGreyBrush">#1a1c20</SolidColorBrush>
		<SolidColorBrush x:Key="SalmonRedBrush">#bc7c7a</SolidColorBrush>
	</Application.Resources>
</Application>
